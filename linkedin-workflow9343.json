    {
      "parameters": {
        "jsCode": "const items = $json[\"items\"];\nconst output = [];\n\nif (items && items.length > 0) {\n    for (const item of items) {\n        if (item.link) {\n            output.push({\n                json: {\n                    url: item.link,\n                },\n            });\n        }\n    }\n}\n\nreturn output;"
      },
