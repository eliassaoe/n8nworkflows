      "parameters": {
        "jsCode": "let start = 0;\nconst limit = $input.first().json.message.content.leadCount;\nconst batchSize = 10;\nconst output = [];\n\nwhile (start < limit) {\n  output.push({ json: { start: start } });\n  start += batchSize;\n}\n\nreturn output;"
      },
